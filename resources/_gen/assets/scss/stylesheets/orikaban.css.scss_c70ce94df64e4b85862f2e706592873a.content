.o_main {
  display: grid;
  align-items: stretch;
  justify-items: stretch;
  grid-template-columns: var(--gutter) 1fr var(--rhythm) 1fr var(--rhythm) 1fr var(--rhythm) 1fr var(--gutter);
  grid-template-rows: 1fr auto; }

@media (max-width: 567px) {
  .o_main__body {
    grid-column: 2/9;
    grid-row: 2/3; } }

@media (min-width: 568px) and (max-width: 767px) {
  .o_main__body {
    grid-column: 2/9;
    grid-row: 2/3; } }

@media (min-width: 768px) and (max-width: 1023px) {
  .o_main__body {
    grid-column: 2/9;
    grid-row: 2/3; } }

@media (min-width: 1024px) and (max-width: 1439px) {

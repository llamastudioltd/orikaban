.l_page {
  background-color: var(--background-1);
  color: var(--foreground-1);
  display: grid;
  grid-template-columns: var(--gutter) 1fr var(--gutter);
  grid-template-rows: var(--gutter) auto calc(var(--rhythm) * 4) 1fr var(--gutter);
  grid-template-areas: ". . ." ". a ." ". . ." "b b b" ". . .";
  min-height: 100vh; }

.l_page__head,
.l_page__body {
  display: grid;
  grid-template-columns: 1fr;
  grid-template-rows: 1fr; }

.l_page__head {
  grid-area: a; }

.l_page__body {
  grid-area: b; }
